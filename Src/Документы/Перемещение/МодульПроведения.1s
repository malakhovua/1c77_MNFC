	Пока Док.ПолучитьДокумент() = 1 Цикл
		Если Док.Вид() = "СписаниеТМЦ" Тогда
			
			Если Док.СозданоВручную = 1 Тогда
				Продолжить;
			КонецЕсли;
			Если Док.ПометкаУдаления() = 1 Тогда
				Док.СнятьПометкуУдаления();
			КонецЕсли;
			ДокСп.НайтиДокумент(Док);
			ДокСп.СделатьНеПроведенным();
			Прервать;
			
		КонецЕсли;
